# RAG System Configuration
# Copy this file to .env and fill in your API keys

# Embedding Provider (openai, cohere, or local)
EMBEDDING_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Cohere Configuration (alternative)
COHERE_API_KEY=your-cohere-api-key-here
COHERE_EMBEDDING_MODEL=embed-english-v3.0

# Local Model Configuration (for sentence-transformers)
LOCAL_EMBEDDING_MODEL=all-MiniLM-L6-v2

# Vector Database Configuration
VECTOR_DB_PATH=./rag_system/database/vector_db

# Indexing Configuration
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
MAX_FILE_SIZE_MB=10

# File Type Configuration
SUPPORTED_EXTENSIONS=.swift,.md,.txt,.json,.plist,.sh,.yml,.yaml

# Retrieval Configuration
TOP_K_RESULTS=5
SIMILARITY_THRESHOLD=0.7

# Performance Configuration
BATCH_SIZE=10
MAX_WORKERS=4
